
@{
    ViewBag.Title = "加入會員";
}

<h2>加入會員</h2>
<br />

<form method="post" action="/Home/CreateAccount">
    <div class="form-group row">
        <label for="inputUserId" class="col-sm-2">會員帳號</label>
        <div class="col-sm-10">
            <input type="text" id="inputUserId" name="UserId" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label for="inputCardName" class="col-sm-2">會員暱稱</label>
        <div class="col-sm-10">
            <input type="text" id="inputnickName" name="nickName" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label for="inputCardLevel" class="col-sm-2">會員密碼</label>
        <div class="col-sm-10">
            <input type="text" id="inputPassword" name="Password" class="form-control" />
        </div>
    </div>
    <input class="btn btn-default" type="submit" value="加入會員" />
</form>

